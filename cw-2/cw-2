
var weight = 44;
var height = 1.57;
var bmi = (weight) / (height * height);
//   إذا كان BMI أقل من 18.6 تطبع الجملة( underweight)
// إذا كان BMI أقل من 24.9 و اكثر من 18.6 تطبع الجملة (Normal)
// إذا كان BMI أكثر من 24.9 تطبع الجملة (Over weight)
  if (bmi < 18.6){
    print("underweight");
  }else if (bmi < 24.9){
    print("Normal");
  }else {
    print("overweight");
  }
  